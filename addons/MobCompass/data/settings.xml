<?xml version="1.1" ?>
<settings>
    <global>
        <bg_blue>0</bg_blue>
        <bg_green>0</bg_green>
        <bg_red>0</bg_red>
        <defaultmode>geo</defaultmode> <!-- options are "geo" or "thf" -->
        <geomode>
            <showbuff>true</showbuff>
        </geomode>
        <thfmode>
            <showangle>always</showangle> <!-- options are "always", "behind" (shows angle when SA = OK), and "never" -->
        </thfmode>
        <txt_blue>0</txt_blue>
        <txt_bold>true</txt_bold>
        <txt_green>0</txt_green>
        <txt_red>255</txt_red>
        <x_pos>260</x_pos>
        <y_pos>550</y_pos>
    </global>
</settings>
