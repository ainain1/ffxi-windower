<?xml version="1.0" ?>
<settings>
    <!--
	This file controls the settings for the FFXIDB plugin.
	Settings in the <global> section apply to all characters.

    <zoom> is a percentage of a 50 yalms radius area.

    <ranges> lets you define a range to show on the map;
    the pattern is <radius>[,<red>,<green>,<blue>][|<radius>[,<red>,<green>,<blue>]];
    ex.: 50,250,0,255|30 will draw a 50 yalms magenta circumference and a 30 yalms black circumference
	-->
    <global>
        <zoom>100</zoom>
        <!-- 0~1000 -->
        <Opacity>75</Opacity>
        <!-- 0~100 -->
        <ShowPC>true</ShowPC>
        <ShowNPC>true</ShowNPC>
        <ShowMob>true</ShowMob>
        <ShowMarker>true</ShowMarker>
        <targetline>true</targetline>
        <Ranges />
        <debugMode>false</debugMode>
